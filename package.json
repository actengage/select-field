    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "change-case": "^4.1.2",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "pascalcase": "^2.0.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
